<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.whatson.prototype"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:logo="@drawable/logowo"
        android:theme="@style/AppTheme" >

        <!-- android:label="@string/app_name" -->
        <activity
            android:name="com.whatson.prototype.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.whatson.prototype.resultDisplayers.ResultListDisplayer"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.whatson.prototype.Providers"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.whatson.prototype.resultDisplayers.TextContentDisplayer"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.whatson.prototype.resultDisplayers.VideoContentDisplayer"
            android:configChanges="orientation"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.whatson.prototype.LoadImages"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>